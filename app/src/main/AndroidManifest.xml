<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.itpvt.forecast">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ican"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.zahidali.forecast_final.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.zahidali.forecast_final.MainActivity" />
        <activity android:name="com.example.zahidali.forecast_final.All_Products_Design" />
        <activity android:name="com.example.zahidali.forecast_final.ProductDetails" />
        <activity android:name="com.example.zahidali.forecast_final.Sub_Categories" />
        <activity android:name="com.example.zahidali.forecast_final.My_Cart" />
        <activity android:name="com.example.zahidali.forecast_final.Check_Out" />
        <activity android:name="com.example.zahidali.forecast_final.Categery" />
        <activity android:name="com.example.zahidali.forecast_final.Nothing" />
        <activity android:name="com.example.zahidali.forecast_final.FullScreenImage" />
        <activity android:name="com.example.zahidali.forecast_final.Avail"></activity>
    </application>


</manifest>